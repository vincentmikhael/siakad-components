{
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@context/*": [
        "./src/context/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@libs/*": [
        "./src/libs/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@views/*": [
        "./src/views/*"
      ]
    }
  }
}
